<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns='http://www.freedesktop.org/standards/shared-mime-info'>
  <mime-type type="application/x-helios-training-session">
    <sub-class-of type="application/json"/>
    <comment>Helios Training Session</comment>
    <glob pattern="*.hts"/>
    <magic priority="50">
      <match type="string" offset="0:100" value="helios-training-session-format"/>
    </magic>
    <icon name="com.cartesiantheatre.helios_training_session"/>
  </mime-type>
</mime-info>

